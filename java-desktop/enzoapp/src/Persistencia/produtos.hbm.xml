<?xml version="1.0" encoding="UTF-8"?>
http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="enzo-food" table="produtos">
        <id column="id" name="id" type="java.lang.Long">
            <generator class="native"></generator>
        </id>
        <property column="nome" name="nome" length="60" type="java.lang.String"/>
        <property column="marca" name="marca" length="60" type="java.lang.String"/>
        <property column="quantidade" name="quantidade" type="java.lang.Integer"/>
        <property column="valorDeCompra" name="valorDeCompra" type="java.lang.Float"/>
        <property column="fornecedor" name="fornecedor" type="java.lang.String"/>
        
    </class>
</hibernate-mapping>